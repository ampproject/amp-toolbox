{
  "extends": [
    "plugin:jsdoc/recommended",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "jsdoc/require-returns-description": "off",
    "jsdoc/newline-after-description": "off",
    "no-undef": "error"
  },
  "env": {
    "node": true,
    "jasmine": true,
    "es2017": true,
    "jest": true
  }
}
